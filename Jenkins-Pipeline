pipeline {
    agent any 
    
    environment{
        PATH = "$PATH:$HOME/bin:$JAVA_HOME:$M2:$M2_HOME"
        }
    stages {
        stage("Git Checkout"){ 
            steps {
             git 'https://github.com/kalanazi1/DevOps-Project-for-Training-'
            }
        }
        stage ("Maven Build"){
            steps{
                sh "mvn package"

        }   
    }
}    
