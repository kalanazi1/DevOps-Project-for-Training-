pipeline {
    agent any 
    
    environment{
        PATH = "/opt/maven/bin:$PATH"
        }
    stages {
        stage("Git Checkout"){ 
            steps {
             git 'https://github.com/kalanazi1/DevOps-Project-for-Training-'
            }
        }
        stage ("Maven Build"){
            steps{
                sh "mvn clean package"
            
            }
        }   
    }
}    
